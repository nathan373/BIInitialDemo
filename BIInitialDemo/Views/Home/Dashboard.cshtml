<style>
    .actualColor {
        background-color: #64a449;
        color: #FFFFFF;
        border: solid 1px #659EC7;
        font-size: x-large;
    }

    .changeColor1 {
        background-color: #e81a1a;
        color: #FFFFFF;
        border: solid 1px #659EC7;
        font-size: x-large;
        cursor: pointer;
    }


    .changeColor2 {
        background-color: #fbe700;
        color: #be1010;
        border: dashed 1px #659EC7;
        font-size: x-large;
        cursor: pointer;
    }

    .columns {
        columns: 4;
    }
</style>

<html>
@{
    ViewBag.Title = "BI Initial Demo";
}
<body>
  

    <div ng-app="plunker" ng-controller="MainCtrl">
        <button ng-click="run=!run">Start / Stop</button>

        <p>duration: 500</p>
        <nvd3 options="options" data="data"></nvd3>

        <p>duration: 0</p>
        <nvd3 options="options1" data="data"></nvd3>

        <br><a href="http://krispo.github.io/angular-nvd3/" target="_blank" style="float: right;">See more</a>
    </div>

    <script type="text/javascript">
        var gridster;

        $(function () {

            var log = document.getElementById('log');


            gridster = $(".gridster ul").gridster({
                widget_base_dimensions: [55, 55],
                widget_margins: [5, 5],
                resize: {
                    enabled: false,

                }
            }).data('gridster');


        });
    </script>
</body>
</html>

